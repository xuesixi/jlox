class Animal {

    static PI = 3.14152926;

    static area(raidus) {
        return PI * 2 * raidus;
    }

    static showNum() {
        print f "the static num is 20";
    }

    static mult(a, b) {
        return a * b;
    }

    init(name, age) {
        this.name = name;
        this.age = age;
    }

    eat() {
        this.run();
        print f "this animal {this.name} is also eating";
    }

    run() {
        print f "the animal {this.name} with age {this.age} is running";
        showNum();
    }

    show() {
        return (this.name, this.age);
    }
}

fun table(x, y) {
    var nums = [x][y];
    for (var i = 0; i < nums.length; i = i + 1) {
        for (var n = 0; n < nums[i].length; n = n + 1) {
            nums[i][n] = f "({i}:{n})";
        }
    }
    return nums;
}

var dog = Animal("anda", 4);
var t = dog.show();
print t;
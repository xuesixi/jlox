class Animal {

    static PI = 3.14152926;

    static area(raidus) {
        return PI * 2 * raidus;
    }

    static showNum() {
        print f "the static num is 20";
    }

    static mult(a, b) {
        return a * b;
    }

    init(name, age) {
        this.name = name;
        this.age = age;
        this.run();
    }

    eat() {
        this.run();
        print f "this animal {this.name} is also eating";
    }

    run() {
        print f "the animal {this.name} with age {this.age} is running";
        showNum();
    }

    show() {
        return (this.name, this.age);
    }
}

class ArrayIterator {
    init(array) {
        this.array = array;
        this.curr = 0;
    }

    hasNext() {
        return this.curr < this.array.length;
    }

    next() {
        this.curr = this.curr + 1;
        return this.array[this.curr-1];
    }
}


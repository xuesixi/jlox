class Animal {

    static PI = 3.14152926;

    static area(raidus) {
        return PI * 2 * raidus;
    }

    static showNum() {
        print f "the static num is 20";
    }

    init(name, age) {
        this.name = name;
        this.age = age;
    }

    eat() {
        this.run();
        print f "this animal {this.name} is also eating";
    }

    run() {
        print f "the animal {this.name} with age {this.age} is running";
        showNum();
    }
}


class ArrayIterator {
    init(array) {
        this.array = array;
        this.curr = 0;
    }

    hasNext() {
        return this.curr < this.array.length();
    }

    next() {
        this.curr = this.curr + 1;
        return this.array[this.curr-1];
    }
}

class ArrayClass {
    iter() {
        return ArrayIterator(this);
    }

    length() {
        return native.len(this);
    }
}

